{% extends 'template.html.twig' %}

{% block title %}Uzytkownicy Index{% endblock %}

{% block body %}
    <h1>Uzytkownicy index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Username</th>
                <th>Password</th>
                <th>Town</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for uzytkownicy in uzytkownicies %}
            <tr>
                <td>{{ uzytkownicy.id }}</td>
                <td>{{ uzytkownicy.username }}</td>
                <td>{{ uzytkownicy.password }}</td>
                <td>{{ uzytkownicy.town }}</td>
                <td>
                    <a href="{{ path('uzytkownicy_show', {'id': uzytkownicy.id}) }}">show</a>
                    <a href="{{ path('uzytkownicy_edit', {'id': uzytkownicy.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('uzytkownicy_new') }}">Create new</a>
{% endblock %}
